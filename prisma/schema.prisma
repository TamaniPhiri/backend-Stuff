// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}



model Student {
  id      Int      @id @default(autoincrement())
  regNo   String
  password String
  lecture Lecture @relation(fields: [password], references: [studentPassword])
}

model Lecture {
  id             Int       @id @default(autoincrement())
  password       String
  studentPassword String @unique
  students  Student[]
}